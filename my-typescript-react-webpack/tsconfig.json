{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "ES2015",
    "target": "es5",
    "lib": ["ES2017", "dom"],
    "sourceMap": true,
    "jsx": "react",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "alwaysStrict": true
  },
  "exclude": [
    "node_modules"
  ]
}
